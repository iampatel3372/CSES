#include <iostream>
#include <vector>
#include <algorithm>
#include <unordered_map>
#define INT_MAX 1e9+7
using namespace std;

int Count;
vector<vector<bool>>vis;
void dfs(string s, int x, int y int lo) {
	if(x < 0 || y < 0 || x >= 7 || y >= 7 || vis[x][y]) return;

	vis[x][y] = true;

	if(x == 6 and y == 6) Count++;

	if(s[lo] == '?') {
		dfs(s, x+1, y, lo+1);
		dfs(s, x, y+1, lo+1);
		dfs(s, x-1, y, lo+1);
		dfs(s, x, y-1, lo+1);
	} else {
		if(s[lo] == 'R') dfs(s, x, y+1, lo+1);
		else if(s[lo] == 'U') dfs(s, x-1, y, lo+1);
		else if(s[lo] == 'D') dfs(s, x+1, y, lo+1);
		else if(s[lo] == 'L') dfs(s, x, y-1, lo+1);
	}
}

int main() {
	string s;
	cin >> s;

	Count = 0;
	vis.resize(7, vector<bool>(7, false));
	dfs(s, 0, 0, 0);

	cout << Count ;
	return 0;
}